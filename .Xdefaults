URxvt*transparent: true
URxvt*shading: 30
URxvt*scrollBar_right: false
URxvt*font: xft: Hack:size=12
URxvt*scrollBar: false
URxvt*perl-ext-common: default, matcher,keyboard-select, resize-font
URxvt*url-launcher: /usr/bin/xdg-open
URxvt*matcher*button: 1
URxvt*keysym*M-m: perl:keyboard-select:activate
URxvt*keysym*M-s: perl:keyboard-select:search
URxvt*keysym*C-j:     resize-font:smaller
URxvt*keysym*C-k:      resize-font:bigger
URxvt*keysym*C-equal:     resize-font:reset
URxvt*keysym*C-minus:  resize-font:show
