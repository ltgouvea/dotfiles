URxvt.shading: 5
URxvt.transparent: false
URxvt.intensityStyles: true
URxvt.scrollBar_right: false
URxvt.font: xft: Source Code Pro for Powerline:size=11
URxvt.scrollBar: false
URxvt.perl-ext-common: default, resize-font, matcher, tabbed, keyboard-select
URxvt.url-launcher: /usr/bin/google-chrome
URxvt.matcher.button: 1
URxvt.keysym.C-j:     resize-font:smaller
URxvt.keysym.C-k:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-minus:  resize-font:show
URxvt.keysym.C-t:  tabbed:new_tab
URxvt.keysym.C-x:  matcher:activate
