command=/usr/share/i3blocks/$BLOCK_NAME
color=#666666
separator_block_width=7
markup=none

[todolist]
interval=10
markup=pango

[spotinfo]
command=/usr/share/i3blocks/spotinfo -i "%artist - %title"
interval=10
markup=pango

[battery]
instance=0
interval=30
markup=pango

[brightness]
command=/usr/share/i3blocks/brightness
interval=2
signal=1
markup=pango

[volume]
command=/usr/share/i3blocks/volume default pulse
interval=2
signal=1
markup=pango

[time]
interval=5
markup=pango
command=/usr/share/i3blocks/time





